<table class="table">
  <thead>
    <tr>
      <th scope="col"><%= header %></th>
      <th scope="col">Average</th>
    </tr>
  </thead>
  <tbody>
    <% best.each do |b| %>
    <tr>
      <td><%= b.name %></td>
      <td><%= round(b.average_rating) %></td>
    </tr>
    <% end %>
  </tbody>
</table>